{
  "name": "airnode-starter",
  "license": "MIT",
  "description": "A starter project for deploying an Airnode and making requests to it",
  "scripts": {
    "build": "truffle compile",
    "create-requester": "node ./scripts/create-requester.js",
    "customize-config": "node ./scripts/customize-config.js",
    "deploy-clients": "node ./scripts/deploy-clients.js",
    "deploy-full-request-client": "node ./scripts/deploy-full-request-client.js",
    "deploy-regular-request-client": "node ./scripts/deploy-regular-request-client.js",
    "deploy-short-request-client": "node ./scripts/deploy-short-request-client.js",
    "derive-designated-wallet-address": "node ./scripts/derive-designated-wallet-address.js",
    "endorse-clients": "node ./scripts/endorse-clients.js",
    "endorse-full-request-client": "node ./scripts/endorse-full-request-client.js",
    "endorse-regular-request-client": "node ./scripts/endorse-regular-request-client.js",
    "endorse-short-request-client": "node ./scripts/endorse-short-request-client.js",
    "fund-designated-wallet": "node ./scripts/fund-designated-wallet.js",
    "fund-master-wallet": "node ./scripts/fund-master-wallet.js",
    "generate-wallet": "node ./scripts/generate-wallet.js",
    "make-full-request": "node ./scripts/make-full-request.js",
    "make-regular-request": "node ./scripts/make-regular-request.js",
    "make-short-request": "node ./scripts/make-short-request.js",
    "update-authorizers": "node ./scripts/update-authorizers.js"
  },
  "dependencies": {
    "@api3/airnode-abi": "^0.1.0",
    "@api3/airnode-admin": "^0.1.5",
    "@api3/airnode-protocol": "^0.1.0"
  },
  "devDependencies": {
    "@truffle/hdwallet-provider": "^1.4.0",
